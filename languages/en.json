    "UpdateNSFW": "Old NSFW: `{oldNSFW}`\nNew NSFW: `{newNSFW}`",
    "UpdateSlowmode": "Old Slowmode: `{oldSlowmode}`\nNew Slowmode: `{newSlowmode}`",
    "UpdateType": "Old Type: `{oldType}`\nNew Type: `{newType}`",
    "UpdateBitrate": "Old Bitrate: `{oldBitrate}`\nNew Bitrate: `{newBitrate}`",
    "UpdateUserLimit": "Old Userlimit: `{oldUserLimit}`\nNew Userlimit: `{newUserLimit}`"
