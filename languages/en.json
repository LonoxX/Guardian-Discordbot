  "channelDelete":{
    "title": "🏚 Channel Deleted",
    "description": "💬 **Channel Name:** `{name}`\n🆔 **Channel ID:** `{id}`\n🔨 **Channel Type:** `{type}`"
  },
  "channelUpdate":{
    "title": "🏚 Channel Updated",
